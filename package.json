{
  "name": "smk-sidebar-generator",
  "version": "3.5.2",
  "description": "WordPress plugin to generate unlimited sidebars",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awps/smk-sidebar-generator.git"
  },
  "author": "Andrei Surdu",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/awps/smk-sidebar-generator/issues"
  },
  "homepage": "https://github.com/awps/smk-sidebar-generator#readme",
  "devDependencies": {
  },
  "tagy": {
    "replace": [
      {
        "files": "src/smk-sidebar-generator.php",
        "from": "Version: \\d+\\.\\d+\\.\\d+",
        "to": "Version: __VERSION__",
        "flags": "g"
      },
      {
        "files": "src/readme.txt",
        "from": "Stable tag: \\d+\\.\\d+\\.\\d+",
        "to": "Stable tag: __VERSION__",
        "flags": "g"
      }
    ]
  }
}
